# Template config for Cloudflare DDNS (copied to /data/cloudflare-ddns.env at runtime)
# Values left empty will be treated as disabled/missing.
# DOMAINS is a comma-separated list of FQDNs.

# Required
CLOUDFLARE_API_TOKEN=
DOMAINS=

# Optional
PROXIED=true
IP4_PROVIDER=
IP6_PROVIDER=

# Notifiers (set *_ENABLED=true and value)
HEALTHCHECKS_ENABLED=false
HEALTHCHECKS=
UPTIMEKUMA_ENABLED=false
UPTIMEKUMA=
SHOUTRRR_ENABLED=false
SHOUTRRR=

# Service control (UI will manage this)
ENABLED=false
